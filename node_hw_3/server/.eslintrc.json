{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["google", "prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "indent": ["error", 4],
        "new-cap": 0,
        "require-jsdoc": 0
    },
    "plugins": ["prettier"]
}